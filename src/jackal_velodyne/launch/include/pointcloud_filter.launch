<launch>
    <arg name="in_topic" default="/velodyne_points"/>
    <arg name="filter" default="false" />
    <arg name="mapping" default="false" />
    <arg name="gt_classify" default="true" />

    <!-- if we are not using ground truth classifications (classes run from 0 -> 6 inclusive) :
    0 - ground
    1 - chair
    2 - moveing person
    3 - still person
    4 - table
    5 - wall
    6 - door
    -->

    <group if="$(arg gt_classify)">
        <group if="$(arg filter)">
        <node pkg="jackal_velodyne" type="pcl_filter_node" name="local_filter">
            <remap from="cloud_in" to="$(arg in_topic)"/>
        
            <remap from="scan" to="/local_planner_points" />
            <rosparam>
            target_frame: base_link
            transform_tolerance: 0.01
            min_height: 0.01
            max_height: 1
            
            angle_min: -3.1415
            angle_max: 3.1415
            angle_increment: 0.01
            scan_time: 0.01
            range_min: 0
            range_max: 30
            use_inf: true
            
            #concurrency_level affects number of pc queued for processing and the number of threadsused
            # 0: Detect number of cores
            # 1: Single threaded
            # 2: inf : Parallelism level
            concurrency_level: 0

            catagories:
                - 0
                - 1
                - 2
                - 3
                - 4
                - 5
                - 6
            </rosparam>
        </node>

        <node pkg="jackal_velodyne" type="pcl_filter_node" name="global_filter">
            <remap from="cloud_in" to="$(arg in_topic)"/>
        
